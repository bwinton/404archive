{
  "manifest_version": 2,

  "name": "404 Archive",
  "version": "0.0.1",
  "description": "Try to find missing pages.",
  "author": "Blake Winton <bwinton@latte.ca>",
  "homepage_url": "https://github.com/bwinton/whimsy",

  "icons": {
      "128": "images/whimsycorn-128.png"
  },

  "applications": {
    "gecko": {
      "id": "404archive@mozilla",
      "strict_min_version": "42.0.0"
    }
  },

  "permissions": [
    "webRequest",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["lib/background.js"],
    "persistent": false
  }

}
